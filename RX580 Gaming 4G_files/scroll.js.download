            $('.fan_2_lg .fans_1').click(
                function test_aorus(){
                    aa_o = $(this).index();
                    console.log(aa_o);
                    if($('.fan_2_part_1 .fan_intro').eq(aa_o).is(':hidden') ){ 
                        $('.fan_2_part_1 .fan_intro').removeClass('been_click');
                        $('.fan_2_lg .number').removeClass('number_active');
                        $('.fan_2_lg .fans_1 .line').css('background-color','#444');                  
                        $('.fan_2_lg .fans_2 .line').css('background-color','#444');
                        $('.fan_2_lg .fan_intro').hide(); 
                        $('.fan_2_lg .fan_intro').eq(aa_o).slideDown(800);
                        $('.fan_2_lg .fan_intro').eq(aa_o).addClass('been_click');
                        $('.fan_2_lg .number').eq(aa_o).addClass('number_active');
                        $('.fan_2_lg .fans_1 .line').eq(aa_o).css('background-color','#ff6400');
                        $('.fan_2_lg .fan_img').hide();
                        $('.fan_2_lg .fan_img_2').hide();
                        $('.fan_2_lg .fan_img').eq(aa_o).fadeIn();
                        return false;
                    }
                }        
            );

